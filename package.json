{
  "name": "kyatsujs",
  "version": "0.1.2",
  "description": "A cool discord.js assistant to create bots in a simple way. Bye bye laborious tasks !",
  "main": "lib/index.js",
  "types": "lib/types/index.d.ts",
  "scripts": {
    "lint": "tslint -p tsconfig.json",
    "prettier": "prettier -w src",
    "save": "git add src/ && git add lib/ && git add typings/",
    "run-builder": "node builder.js",
    "build": "npm run run-builder && tsc && npm run save"
  },
  "keywords": [
    "discord",
    "bot"
  ],
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib",
    "package.json",
    "README.md",
    "tsconfig.json"
  ],
  "author": "Elouann Hosta",
  "license": "ISC",
  "devDependencies": {
    "prettier": "^2.8.4",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.24.2"
  },
  "dependencies": {
    "discord-api-types": "^0.37.38",
    "discord.js": "^14.8.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/elouann-h/kyatsu.git"
  }
}
